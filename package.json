{
  "name": "gemini-leetcode-assist",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build && vite build -c vite.content-script.config.ts",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "test": "jest --testPathIgnorePatterns=e2e",
    "test:e2e": "xvfb-run -a --server-args=\"-screen 0 1920x1080x24\" jest --config=jest.e2e.config.ts --detectOpenHandles",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "check:updates": "npm-check-updates",
    "check:audit": "npm audit",
    "check:licenses": "license-checker --summary"
  },
  "dependencies": {
    "@google/generative-ai": "^0.24.1",
    "@reduxjs/toolkit": "^2.9.0",
    "@tailwindcss/container-queries": "^0.1.1",
    "lucide-react": "^0.544.0",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-draggable": "^4.5.0",
    "react-redux": "^9.2.0",
    "react-resizable": "^3.0.5",
    "redux": "^5.0.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.33.0",
    "@tailwindcss/cli": "^4.1.13",
    "@tailwindcss/nesting": "^0.0.0-insiders.565cd3e",
    "@tailwindcss/postcss": "^4.1.13",
    "@tailwindcss/vite": "^4.1.13",
    "@testing-library/jest-dom": "^6.8.0",
    "@testing-library/react": "^16.3.0",
    "@types/chrome": "^0.1.12",
    "@types/jest": "^30.0.0",
    "@types/react": "^19.1.10",
    "@types/react-dom": "^19.1.7",
    "@types/react-redux": "^7.1.34",
    "@vitejs/plugin-react": "^5.0.0",
    "autoprefixer": "^10.4.21",
    "eslint": "^9.33.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "globals": "^16.3.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^30.1.3",
    "jest-environment-jsdom": "^30.1.2",
    "license-checker": "^25.0.1",
    "npm-check-updates": "^18.1.0",
    "postcss": "^8.5.6",
    "postcss-import": "^16.1.1",
    "prettier": "^3.6.2",
    "puppeteer": "^24.19.0",
    "tailwindcss": "^4.1.13",
